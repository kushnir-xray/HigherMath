    "from scipy.optimize import fsolve\n",
    "def equitations(p):\n",
    "    x, y = p\n",
    "    return y - x**2 + 1, np.exp(x) + x*(1-y) - 1\n",
    "\n",
    "x1, y1 = fsolve(equitations, (0, 0))\n",
    "\n",
    "print(x1, y1)"
   ]
