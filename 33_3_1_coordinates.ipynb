    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "\n",
    "def polar_to_cartesian(R, a):\n",
    "    x = R * np.cos(np.radians(a))\n",
    "    y = R * np.sin(np.radians(a))\n",
    "    return x, y\n",
    "print(polar_to_cartesian(5, 10))"
   ]
